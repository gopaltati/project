# @title CustomerID

from matplotlib import pyplot as plt
df['CustomerID'].plot(kind='hist', bins=20, title='CustomerID')
plt.gca().spines[['top', 'right',]].set_visible(False)

# @title Age

from matplotlib import pyplot as plt
